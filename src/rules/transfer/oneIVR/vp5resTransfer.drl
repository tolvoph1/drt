*/
rule "02_BusinessNumber_Unique"
	no-loop
	salience 19000
	when
	  cp: CallProfile ( businessnumber != null )
	  bnb : BusinessNumberBean( businessNumber == BusinessNumberBean.EMPTY )
	then
	  bnb.setBusinessNumber((String)cp.getBusinessnumber());
end
