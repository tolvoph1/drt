rule "04a_GenerateProductClusterTV"
	no-loop
	salience 10000
	when
		custProducts : CustomerProducts( ) 
		eval ( 
			custProducts.contains("BluewinTV") ||
			custProducts.contains("ALL:Trio Bundle") 
		)
		r5b: R5RulesBean()
	then
		r5b.setHasProductClusterTV(true);
end
