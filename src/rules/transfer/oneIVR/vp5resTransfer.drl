rule "05_PopulateAgeDetails"
	no-loop
	salience 10000
	when
	  customerProfile: CustomerProfile ( )
	  serviceconfig: ServiceConfigurationMap()
	  eval (
			!StringUtils.isEmpty(customerProfile.getAge()) &&
			!StringUtils.isEmpty((String)serviceconfig.get("vp.res.caller.MaxAge")) &&
			!StringUtils.isEmpty((String)serviceconfig.get("vp.res.caller.BestAgeStart"))
		)
		r5b: R5RulesBean()
	then
		r5b.setCustomerAge(customerProfile.getAge());
		r5b.setMaxAge((String)serviceconfig.get("vp.res.caller.MaxAge"));
		r5b.setBestAge((String)serviceconfig.get("vp.res.caller.BestAgeStart"));
end

rule "07_PopulateYoungsterBestage"
	no-loop
	salience 10000
	when
		customerProfile: CustomerProfile ( )
		serviceconfig: ServiceConfigurationMap()
		r5b: R5RulesBean()
		eval (
			  !StringUtils.isEmpty(customerProfile.getAge()) &&
			  !StringUtils.isEmpty((String)serviceconfig.get("vp.res.YoungsterBestAge.maxAge"))
			 )
	then
		r5b.setYoungsterBestAge(new Integer(customerProfile.getAge()).compareTo(new Integer((String)serviceconfig.get("vp.res.YoungsterBestAge.maxAge")))<0);
end
			  


